{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build and Run Clip Scraper",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"build"
			],
			"group": "build",
			"problemMatcher": [
				"$tsc"
			],
			"isBackground": false
		},
		{
			"label": "Run Development",
			"type": "shell",
			"command": "npm",
			"args": ["run", "dev"],
			"group": "build",
			"isBackground": false,
			"problemMatcher": []
		},
		{
			"label": "Watch Mode",
			"type": "shell",
			"command": "npm",
			"args": ["run", "watch"],
			"group": "build",
			"isBackground": true,
			"problemMatcher": []
		}
	]
}